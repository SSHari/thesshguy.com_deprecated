    "gatsby-plugin-mdx": "^3.4.0",
    "gatsby-plugin-postcss": "^5.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "postcss": "^8.4.5",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.11"
  },
  "devDependencies": {
    "ssh-dev-scripts": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
