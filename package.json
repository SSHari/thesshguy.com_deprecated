{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "npm run format:files -- --write",
    "format:files": "prettier --ignore-path .gitignore .",
    "format:check": "npm run format:files -- --check",
    "lint": "eslint .",
    "prepare": "husky install",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "validate": "npm-run-all --parallel format:check lint test typecheck"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "gatsby": "^4.0.0-zz-next.1",
    "gatsby-plugin-postcss": "^5.0.0",
    "postcss": "^8.3.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.2.17"
  },
  "devDependencies": {
    "ssh-dev-scripts": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
